{
  "name": "xb-ui",
  "version": "1.1.0",
  "description": "SchoolPal's UI components",
  "main": "lib/xb-ui.common.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "create:component": "node tools/create-component",
    "create:componentMd": "node tools/create-componentMd",
    "dist:style": "node build/gen-cssfile && gulp --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/dist lib/theme-chalk",
    "dist:component": "webpack --config build/webpack.component.js --mode production",
    "dist": "webpack --config build/webpack.production.js --mode production && webpack --config build/webpack.component.js --mode production && npm run dist:style",
    "format": "prettier --write \"packages/**/*.js\" \"packages/**/*.vue\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "packages/**/*.{js,json,css,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "style": "lib/theme-chalk/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchoolPalCoder/Xb-Basic-Components.git"
  },
  "keywords": [
    "vue",
    "xb",
    "ui",
    "components",
    "vuepress"
  ],
  "author": "SchoolPal Coder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SchoolPalCoder/Xb-Basic-Components/issues"
  },
  "homepage": "https://github.com/SchoolPalCoder/Xb-Basic-Components#readme",
